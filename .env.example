# Environment Configuration Example
# Copy to .env and modify as needed

# Environment: local, dev, prod
ENVIRONMENT=local

# Redis Configuration
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_DB=0

# Logging
LOG_LEVEL=INFO

# OpenSky Credentials (optional)
OPENSKY_USERNAME=
OPENSKY_PASSWORD=

# Redis Commander (for redis-stack)
REDIS_COMMANDER_USER=admin
REDIS_COMMANDER_PASSWORD=change_me_in_production

# Timezone
TZ=America/Los_Angeles

# Config file selection
CONFIG_FILE=collectors-${ENVIRONMENT}.yaml